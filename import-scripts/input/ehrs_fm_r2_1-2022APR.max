    <object>
      <id>715</id>
      <name>CPS.1.6.1#02</name>
      <alias />
      <notes>The system SHALL provide the ability to extract the identity of persons related by genealogy to the patient.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>713</parentId>
      <modified>2014-04-16T00:00:00</modified>
      <tag name="Row" value="707"></tag>
      <tag name="Optionality" value="SHALL"></tag>
      <tag name="Conditional" value="N"></tag>
