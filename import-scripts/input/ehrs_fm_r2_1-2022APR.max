      <tag name="Reference.FunctionID" value="S.3.5.4"></tag>
      <tag name="Reference.CriteriaID" value="2"></tag>
      <tag name="Reference.ChangeInfo" value="Modified"></tag>
    </object>
    <object>
      <id>725</id>
      <name>CPS.1.6.4#03</name>
      <alias />
      <notes>The system MAY provide the ability to render information regarding persons related to the patient other than by genealogy, insurance, and/or living situation according to scope of practice, organizational policy, and/or jurisdictional law.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>722</parentId>
