    <object>
      <id>498</id>
      <name>CP.6.1#09</name>
      <alias />
      <notes>The system SHALL conform to function [[CPS.4.2.2]] (Support for Patient-Specific Dosing and Warnings) in order to determine and render other potential adverse reactions, when rendering medication administration information.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>489</parentId>
      <modified>2014-09-08T00:00:00</modified>
      <tag name="Row" value="491"></tag>
