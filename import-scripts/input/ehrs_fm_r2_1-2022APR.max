    <object>
      <id>2666</id>
      <name>TI.8#04</name>
      <alias />
      <notes>The system MAY provide the ability to backup EHR information according to a defined schedule of storage media rotation.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>2662</parentId>
      <modified>2014-04-16T00:00:00</modified>
