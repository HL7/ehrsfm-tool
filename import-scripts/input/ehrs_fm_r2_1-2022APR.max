    <object>
      <id>1627</id>
      <name>AS.7.5#03</name>
      <alias />
      <notes>The system MAY provide the ability to link transfer facility demographic information to the transfer patient.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>1624</parentId>
