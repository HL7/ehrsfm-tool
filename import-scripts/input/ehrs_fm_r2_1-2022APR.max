    <object>
      <id>1477</id>
      <name>AS.2.5#03</name>
      <alias />
      <notes>The system SHOULD provide the ability to manage separate status options for multiple healthcare program.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>1474</parentId>
