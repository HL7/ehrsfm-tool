    <object>
      <id>1069</id>
      <name>CPS.5#06</name>
      <alias />
      <notes>The system SHALL tag critical value results that have not been acknowledged.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>1063</parentId>
