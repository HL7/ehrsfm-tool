      <tag name="Owner" value=""></tag>
      <tag name="DBVersion" value=""></tag>
    </object>
    <object>
      <id>727</id>
      <name>CPS.1.7#01</name>
      <alias />
      <notes>The system SHOULD conform to function [[CPS.1.7.1]] (Support for Patient and Family Preferences).</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>726</parentId>
      <modified>2014-09-08T00:00:00</modified>
      <tag name="Row" value="719"></tag>
      <tag name="Optionality" value="SHOULD"></tag>
      <tag name="Conditional" value="N"></tag>
