    <object>
      <id>2663</id>
      <name>TI.8#01</name>
      <alias />
      <notes>The system SHALL provide the ability to backup and recover EHR information according to scope of practice, organizational policy, and/or jurisdictional law.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>2662</parentId>
      <modified>2014-04-16T00:00:00</modified>
