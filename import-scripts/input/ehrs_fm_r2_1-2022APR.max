    <object>
      <id>1510</id>
      <name>AS.4.1#02</name>
      <alias />
      <notes>The system MAY provide the ability to render and tag registry information as reviewed and the information's related assessment of validity or applicability for clinical, financial or administrative activities.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>1508</parentId>
