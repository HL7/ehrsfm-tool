      <tag name="Reference.CriteriaID" value="1"></tag>
      <tag name="Reference.ChangeInfo" value="Modified"></tag>
    </object>
    <object>
      <id>1318</id>
      <name>POP.3#02</name>
      <alias />
      <notes>The system SHALL provide the ability to render a response notification to the care providers or care managers within a cared-for population that a health risk notification was received.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>1316</parentId>
