      <tag name="DBVersion" value=""></tag>
    </object>
    <object>
      <id>721</id>
      <name>CPS.1.6.3#01</name>
      <alias />
      <notes>The system MAY provide the ability to render living situation related information.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>720</parentId>
      <modified>2014-04-16T00:00:00</modified>
