      <tag name="Reference.ChangeInfo" value="Modified"></tag>
    </object>
    <object>
      <id>1065</id>
      <name>CPS.5#02</name>
      <alias />
      <notes>The system SHOULD provide the ability to render trend results.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>1063</parentId>
      <modified>2014-04-16T00:00:00</modified>
      <tag name="Row" value="1060"></tag>
