    <object>
      <id>1369</id>
      <name>POP.7#02</name>
      <alias />
      <notes>The system MAY provide the ability to render information that will promote the validation of the public health education material prior to update.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>1367</parentId>
      <modified>2014-04-16T00:00:00</modified>
      <tag name="Row" value="1363"></tag>
