    <object>
      <id>1329</id>
      <name>POP.4#04</name>
      <alias />
      <notes>The system SHALL provide the ability to determine and render a status report regarding the compliance of the set of all patients who are the target of a health risk alert.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>1325</parentId>
