    <object>
      <id>1175</id>
      <name>CPS.9.2#02</name>
      <alias />
      <notes>The system SHALL provide the ability to integrate scanned documents from providers into the patient record.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>1173</parentId>
      <modified>2014-04-16T00:00:00</modified>
