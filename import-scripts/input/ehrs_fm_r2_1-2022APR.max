    <object>
      <id>1322</id>
      <name>POP.3#06</name>
      <alias />
      <notes>The system SHOULD determine and present suggestions to the care provider indicating an appropriate course of action regarding a population health risk notification.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>1316</parentId>
