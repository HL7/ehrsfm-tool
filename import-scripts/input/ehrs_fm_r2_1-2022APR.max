      <tag name="Optionality" value="SHOULD"></tag>
      <tag name="Conditional" value="N"></tag>
      <tag name="Dependent" value="N"></tag>
    </object>
    <object>
      <id>1359</id>
      <name>POP.6.3#02</name>
      <alias />
      <notes>The system SHOULD provide the ability to capture necessary data (e.g., patient satisfaction feedback) supporting organizational efforts to improve the quality of healthcare and patient satisfaction.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>1357</parentId>
