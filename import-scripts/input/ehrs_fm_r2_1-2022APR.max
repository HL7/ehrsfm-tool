    </object>
    <object>
      <id>1338</id>
      <name>POP.6.1#01</name>
      <alias />
      <notes>The system SHOULD provide the ability to render data required to evaluate patient outcomes.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>1337</parentId>
      <modified>2014-04-16T00:00:00</modified>
