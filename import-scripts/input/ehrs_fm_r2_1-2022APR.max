    <object>
      <id>502</id>
      <name>CP.6.1#13</name>
      <alias />
      <notes>The system SHOULD provide the ability to render any clinical interventions or assessments required prior to medication administration.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>489</parentId>
      <modified>2014-04-16T00:00:00</modified>
      <tag name="Row" value="495"></tag>
