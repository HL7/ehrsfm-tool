    <object>
      <id>1483</id>
      <name>AS.2.6#05</name>
      <alias />
      <notes>The system MAY provide the ability to enter, import or receive information that documents the patient's expressed selection of privacy preferences related to the disclosure of information identified by its content type (e.g., related diagnosis or payment method), and a specific purpose.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>1478</parentId>
      <modified>2014-04-16T00:00:00</modified>
      <tag name="Row" value="1476"></tag>
      <tag name="Optionality" value="MAY"></tag>
