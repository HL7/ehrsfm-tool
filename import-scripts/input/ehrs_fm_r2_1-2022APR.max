    <object>
      <id>1482</id>
      <name>AS.2.6#04</name>
      <alias />
      <notes>The system SHOULD provide the ability to render an accounting of any patient identifiable information disclosed to other providers.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>1478</parentId>
      <modified>2014-04-16T00:00:00</modified>
