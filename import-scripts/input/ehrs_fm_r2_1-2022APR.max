    <object>
      <id>1319</id>
      <name>POP.3#03</name>
      <alias />
      <notes>The system SHALL provide the ability to capture, maintain and render notification of a health risk within a cared-for population from public health authorities or other external authoritative sources.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>1316</parentId>
