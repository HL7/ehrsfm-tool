    <object>
      <id>1328</id>
      <name>POP.4#03</name>
      <alias />
      <notes>The system SHALL determine and render a list of those patients who have not received appropriate action in response to a health risk alert.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>1325</parentId>
