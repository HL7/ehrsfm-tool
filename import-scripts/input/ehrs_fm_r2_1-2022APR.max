    <object>
      <id>1321</id>
      <name>POP.3#05</name>
      <alias />
      <notes>The system SHOULD provide the ability to transmit notifications to patients, directly or indirectly, who are described by the health risk alert.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>1316</parentId>
