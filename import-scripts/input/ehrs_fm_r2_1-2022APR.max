      <tag name="Optionality" value="SHOULD"></tag>
      <tag name="Conditional" value="N"></tag>
      <tag name="Dependent" value="N"></tag>
    </object>
    <object>
      <id>558</id>
      <name>CP.6.3#05</name>
      <alias />
      <notes>The system SHALL provide the ability to render the information necessary to adminster the treatment (e.g., body site, time and frequency).</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>553</parentId>
      <modified>2014-04-16T00:00:00</modified>
