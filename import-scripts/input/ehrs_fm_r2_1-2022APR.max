      <tag name="Optionality" value="SHALL"></tag>
      <tag name="Conditional" value="N"></tag>
      <tag name="Dependent" value="N"></tag>
    </object>
    <object>
      <id>559</id>
      <name>CP.6.3#06</name>
      <alias />
      <notes>The system SHALL provide the ability to capture, maintain, and render information regarding multiple body sites where treatments are scheduled to be administered.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>553</parentId>
      <modified>2017-03-27T00:00:00</modified>
