<fo:block text-align="center">711</fo:block>
</fo:table-cell>
</fo:table-row>
<fo:table-row border="solid 0.5pt black" padding-left=".5em" keep-with-next.within-page="always" border-top="solid 1.25pt black">
<fo:table-cell border-right="solid 0.5pt black" keep-together="always">
<fo:block border-bottom="solid 0.2 black" space-before=".7em" margin-left=".5em" keep-with-next.within-page="always" id="CPS.1.6.3">CPS.1.6.3</fo:block>
