<fo:block border-bottom="solid 0.2 black" space-before=".7em" margin-left=".5em" keep-with-next.within-page="always" id="CPS.3.12">CPS.3.12</fo:block>
<fo:block margin-left=".5em">Function</fo:block>
</fo:table-cell>
<fo:table-cell border-right="solid 0.5pt black" display-align="center">
<fo:block display-align="center" text-align="center" font-size="1.2em" font-weight="bolder"> Manage Health Information Record Quality</fo:block>
</fo:table-cell>
<fo:table-cell border-right="solid 0.5pt black" display-align="center">
<fo:block text-align="center"/>
</fo:table-cell>
<fo:table-cell border-right="solid 0.5pt black" display-align="center">
<fo:block display-align="center" text-align="center">NC</fo:block>
</fo:table-cell>
