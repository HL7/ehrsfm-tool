<fo:table-cell border-right="solid 0.5pt black" keep-together="always">
<fo:block border-bottom="solid 0.2 black" space-before=".7em" margin-left=".5em" keep-with-next.within-page="always" id="CPS.1.6.4">CPS.1.6.4</fo:block>
<fo:block margin-left=".5em">Function</fo:block>
</fo:table-cell>
<fo:table-cell border-right="solid 0.5pt black" display-align="center">
<fo:block display-align="center" text-align="center" font-size="1.2em" font-weight="bolder"> Related by Other Means</fo:block>
</fo:table-cell>
<fo:table-cell border-right="solid 0.5pt black" display-align="center">
