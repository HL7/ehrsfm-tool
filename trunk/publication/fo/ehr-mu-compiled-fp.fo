<fo:table-cell border-right="solid 0.5pt black" display-align="center">
<fo:block display-align="center" text-align="center" font-size="1.2em" font-weight="bolder"> Record Synchronization</fo:block>
</fo:table-cell>
<fo:table-cell border-right="solid 0.5pt black" display-align="center">
<fo:block text-align="center">RI.2</fo:block>
</fo:table-cell>
<fo:table-cell border-right="solid 0.5pt black" display-align="center">
<fo:block display-align="center" text-align="center">NC</fo:block>
</fo:table-cell>
