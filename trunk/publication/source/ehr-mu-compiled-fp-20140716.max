      <tag name="Reference.CriterionID" value="3" />
      <tag name="Reference.ChangeIndicator" value="C" />
      <tag name="Priority" value="EN" />
    </object>
    <object>
      <id>35db06af-758f-40e3-828a-74658d4fd074</id>
      <name>AS.2.6#04</name>
      <notes>The system SHOULD provide the ability to render an accounting of any patient identifiable information disclosed to other providers.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>a50d561c-e9c9-40b1-b78c-1e315fcdc566</parentId>
