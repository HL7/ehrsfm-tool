      <tag name="Reference.FunctionID" value="CP.4" />
      <tag name="Reference.CriterionID" value="2" />
      <tag name="Reference.ChangeIndicator" value="NC" />
      <tag name="Priority" value="EN" />
    </object>
    <object>
      <id>51d4fa44-c21f-49d3-a7a3-822398190141</id>
      <name>CP.4#04</name>
      <notes>The system SHALL provide the ability to manage the status of an order (e.g., open, completed, in process).</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>f922c96f-208e-4a6d-be42-55593736f95e</parentId>
