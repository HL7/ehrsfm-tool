      <tag name="Priority" value="EN" />
    </object>
    <object>
      <id>12887d10-68f8-45a1-b253-08ac9ba0ba77</id>
      <name>CP.4#02</name>
      <notes>The system SHALL provide the ability to manage the creation, renewal, modification and discontinuation of orders.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>f922c96f-208e-4a6d-be42-55593736f95e</parentId>
      <tag name="Optionality" value="SHALL" />
      <tag name="Conditional" value="N" />
